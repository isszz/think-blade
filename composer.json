{
    "name": "isszz/think-blade",
    "description": "thinkphp8 blade view engine",
    "license": "MIT",
    "authors": [
        {
            "name": "isszz",
            "email": "isszz@qq.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*",
        "ramsey/uuid": "^4.7",
        "doctrine/inflector": "^2.0",
        "symfony/finder": "^4.3.4",
        "topthink/framework": "8.*"
    },
    "autoload": {
        "files": [
            "src/helper.php",
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "think\\blade\\": "src",
            "Illuminate\\Container\\": "src/Container",
            "Illuminate\\Contracts\\": "src/Contracts",
            "Illuminate\\Support\\": "src/Support",
            "Illuminate\\Filesystem\\": "src/Filesystem",
            "Illuminate\\View\\": "src/View"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "think": {
            "services": [
                "think\\blade\\BladeService"
            ]
        },
        "branch-alias": {
            "dev-master": "dev-dev"
        }
    }
}
